<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>ZULCOM - Administrador</title>
  <link rel="stylesheet" href="/css/dashboard/navbar.css" />
  <link rel="stylesheet" href="/css/dashboard/dashboard.css" />
<link rel="stylesheet" href="/css/ver_rol_pago/styles_ver_rol.css" />

  


</head>
<body>
  <div class="dashboard-container">
    <%- include('../partials/navadministrador') %>
    

    <main class="main-content">
      <header class="content-header">
        <h1>PANEL ADMINISTRATIVO</h1>
        <div class="user-actions">
          <div>
            <a href="/auth/change-password">
              <img src="/img/logo-perfil.png" alt="Logo Perfil" class="img-perfil" />
            </a>
          </div>
          <% if (user && user.nombres && user.apellidos) { %>
            <span class="user-name">Administrador: <%= user.nombres %> <%= user.apellidos %></span>
          <% } %>
          <a href="/auth/logout" class="logout-btn" onclick="return confirm('¿Estás seguro de cerrar sesión?')">
            Cerrar Sesión
          </a>
        </div>
      </header>

      <!-- Contenido específico del dashboard administrativo -->
      <div class="dashboard-content">
        <div id="content">
          <h2></h2>
          <p></p>
        </div>
      </div>
    </main>
  </div>
 <script src="/js/ver_roles/script.js"></script>



 /body>
</html>
